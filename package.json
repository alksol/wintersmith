{
  "name": "wintersmith",
  "version": "2.1.1",
  "author": "Johan Nordberg <code@johan-nordberg.com>",
  "description": "A flexible static site generator.",
  "license": "MIT",
  "main": "./lib/",
  "keywords": [
    "static",
    "generator",
    "markdown",
    "jade"
  ],
  "bin": {
    "wintersmith": "./bin/wintersmith"
  },
  "scripts": {
    "prepublish": "node ./bin/dev/compile_coffee"
  },
  "directories": {
    "lib": "./lib/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jnordberg/wintersmith.git"
  },
  "dependencies": {
    "async":          ">=0.2.9 <0.10 < 1.0",
    "chokidar":       "^1.0.0-rc3",
    "coffee-script":  "^1.6.3",
    "colors":         "^1.0.3",
    "highlight.js":   "^8.0.0",
    "jade":           "^1.1.5",
    "js-yaml":        "^3.0.1",
    "marked":         "^0.3.0",
    "mime":           "^1.2.11",
    "minimatch":      "^2.0.1",
    "mkdirp":         "^0.5.0",
    "ncp":            "^1.0.1",
    "npm":            "^2.5.0",
    "optimist":       "^0.6.0",
    "rimraf":         "^2.2.6",
    "server-destroy": "^1.0.0",
    "slugg":          "^0.1.2",
    "winston":        "^0.9.0"
  },
  "devDependencies": {
    "shelljs": "^0.3.0"
  },
  "engine": {
    "node": ">=0.6"
  }
}
